<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
        "http://getahead.org/dwr/dwr30.dtd">
<!-- DWR配置文件的根元素是dwr -->
<dwr>
    <allow>
        <!-- 使用new创建一个JavaScript对象，该对象名为CallAdsFuncs -->
        <create creator="new" javascript="CallAdsFuncs" scope="application">
            <param name="class" value="twcat.CallAdsFuncs"/>
        </create>
    </allow>
</dwr>